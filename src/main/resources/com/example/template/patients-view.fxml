<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>


<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.template.PatientsController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <HBox>
        <Label text="Pacienți în așteptare: "/>
        <Label text="0" fx:id="pending"/>
    </HBox>

    <ListView fx:id="list"/>
    <ChoiceBox fx:id="choice">
    </ChoiceBox>
    <CheckBox text="Necesită ventilație" fx:id="o2"/>
    <Button fx:id="addBtn" text="Trimite" onAction="#addPatient"/>
</VBox>
